{
  "name": "misha-api",
  "scripts": {
    "dev": "bun run --hot src/index.ts | pino-pretty",
    "test": "bun test | pino-pretty",
    "lint": "pnpm biome lint --write src",
    "format": "pnpm biome format --write src",
    "check": "pnpm biome check src",
    "check:knip": "bunx --bun knip",
    "typecheck": "tsc --noEmit",
    "type:generate": "rm -rf generated_types && tsc --project tsconfig.types.json && mv ./generated_types/robotApi.d.ts ../front/src/api.d.ts && rm -rf generated_types",
    "db:start": "docker compose -f postgres.yml up -d",
    "db:stop": "docker compose -f postgres.yml down",
    "db:clean": "docker compose -f postgres.yml down -v",
    "db:generate": "dotenv -c pnpm drizzle-kit generate",
    "db:migrate": "dotenv -c pnpm drizzle-kit migrate",
    "db:studio": "dotenv -c pnpm drizzle-kit studio",
    "email-preview": "email preview ./src/emails"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.18.3",
    "@literalai/client": "^0.1.5",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@openrouter/ai-sdk-provider": "^0.0.6",
    "@reown/appkit-siwe": "^1.6.5",
    "@roamhq/wrtc-linux-x64": "^0.8.1",
    "@scalar/hono-api-reference": "^0.5.169",
    "cat-ai": "workspace:*",
    "cat-logger": "workspace:*",
    "cat-media-gen": "workspace:*",
    "cat-storage": "workspace:*",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
    "hono": "^4.6.19",
    "ioredis": "^5.4.2",
    "join-images": "^1.1.5",
    "jsx-email": "^2.7.0",
    "lucia": "^3.2.2",
    "nodemailer": "^6.10.0",
    "openai": "^4.81.0",
    "oslo": "^1.2.1",
    "postgres": "^3.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "4.1.1",
    "robot-onchain": "workspace:*",
    "robot-sdk": "workspace:*",
    "sharp": "^0.33.5",
    "viem": "^2.22.17",
    "znv": "^0.4.0",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "^1.2.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.4",
    "pino-pretty": "^13.0.0",
    "redis-memory-server": "^0.12.1"
  }
}
